:plain
  <script>
    if (location.href.search(/#/) !== -1) location.href = '/';
  </script>

  <div data-role='page' id='home_page' data-add-back-btn='false'>

    <div data-role='header' data-position='fixed'>
      <h1>CodeMash</h1>
    </div><!-- /header -->

    <div data-role='content'>
      <ul data-role='listview'>
        <li><a href='#precompiler_sessions'>Precompiler</a></li>
        <li><a href='#thursday_sessions'>Thursday</a></li>
        <li><a href='#friday_sessions'>Friday</a></li>
      </ul>
    </div><!-- /content -->

  </div><!-- /page -->

  <script type='text/template' id='sessions_list_template'>
    <div data-role='header' data-position='fixed'>
      <h1><%= navBarTitle %></h1>
    </div><!-- /header -->

    <div data-role='content'>
      <ul data-role='listview' class='sessions_list'></ul>
    </div><!-- /content -->
  </script>

  <script type='text/template' id='sessions_list_item_template'>
    <a href='<%= href %>'>
      <div class='title'><%= title %></div>
      <div class='wherewhen'>
        <%= when %>
        <%= room %>
      </div>
      <div class='speaker'><%= speakerName %></div>
    </a>
  </script>

  <script type='text/template' id='session_list_page_template'>
    <div data-role='page'>

      <div data-role='header' data-position='fixed'>
        <h1>Session List</h1>
      </div><!-- /header -->

      <div data-role='content'>
        <ul data-role='listview' class='sessions_list'></ul>
      </div><!-- /content -->

    </div><!-- /page -->
  </script>

